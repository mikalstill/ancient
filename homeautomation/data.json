{
  "bg_colour": "#FFFFFF",

  "title":{
    "text":  "Sensors",
    "style": "{font-size: 12px; color:#0000ff; font-family: Verdana; text-align: center;}"
  },
 
  "elements":[
    {{elements}}
  ],
 
  "x_axis":{
    "stroke":      1,
    "tick_height": 10,
    "min":         {{x_min}},
    "max":         {{x_max}},
    "steps":       {{x_step}},
    "labels":{
       "text":     "{{x_format}}",
       "steps":    {{x_step}},
       "visible-steps": 1,
       "rotate":        90
    }
   },
 
  "y_axis":{
    "stroke":      4,
    "steps":       {{y_step}},
    "tick_length": 1,
    "offset":      0,
    "min":	   {{y_min}},
    "max":         {{y_max}}
  }
}
