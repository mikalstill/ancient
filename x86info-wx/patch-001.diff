+
+void Identify_IDT (struct cpudata *cpu)
+{
+	char *nameptr;
+	unsigned long eax, ebx, ecx, edx;
+
+	nameptr = cpu->name;
+
+	cpu->vendor = VENDOR_CENTAUR;
+
+	/* Do standard stuff */
+	if (cpu->maxi < 1)
+		return;
